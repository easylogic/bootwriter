- if (typeof skinText != 'undefined' && skinText) 
  style(type='text/css')!= skinText
div(id= cid)
  - if (!isContainer)  
    .logic-comp-viewpoint(data-cid= cid, id= cid)
      .viewText!= viewText
  - if (isContainer)
    .logic-comp-childpoint(data-cid= cid, class= (rowType == 'fluid') ? 'row-fluid' : 'row')